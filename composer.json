    "keywords": [
        "cloudinary",
        "image",
        "cdn"
    ],
    "require": {
        "bolt/bolt": "^3.0"
    },
    "license": "",
    "authors": [
        {
            "name": "Condé Nast Verlag GmbH",
            "email": "tobias.bulla@condenast.de",
            "homepage": "https://techblog.condenast.de"
        },
        {
            "name": "Condé Nast Verlag GmbH",
            "email": "rosen.ralev@condenast.de",
            "homepage": "https://techblog.condenast.de"
